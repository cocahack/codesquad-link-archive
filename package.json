    "test": "jest",
    "test:cov": "jest --coverage",
    "start": "tsc && node dist/index.js"
  },
  "dependencies": {
    "@koa/router": "^8.0.8",
    "@slack/web-api": "^5.8.1",
    "@types/jsonwebtoken": "^8.3.9",
    "ioredis": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "kompression": "^1.0.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "mongoose": "^5.9.12",
    "ramda": "^0.27.0",
    "source-map-support": "^0.5.19",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/ioredis": "^4.16.1",
    "@types/jest": "^25.1.3",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/koa__router": "^8.0.2",
    "@types/kompression": "^1.0.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/mongoose": "^5.7.15",
    "@types/node": "^13.7.7",
    "@types/ramda": "^0.27.4",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4"
  },
  "author": "MCC",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup-tests.ts"
    ]
  }
